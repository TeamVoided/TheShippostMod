{
  "schemaVersion": 1,
  "id": "shippost",
  "version": "${version}",
  "name": "The Shippost Mod",
  "description": "Just a normal mod dont worry!",
  "authors": [
    "Ender",
    "Astra"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "MIT",
  "icon": "assets/shippost/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "org.teamvoided.shippost.TheShippostMod"
      }
    ]
  },
  "mixins": [
    "shippost.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.21",
    "fabric": "*",
    "minecraft": "1.20.x",
    "java": ">=17",
    "fabric-language-kotlin": ">=1.9.5+kotlin.1.8.22"
  },
  "suggests": {
    "another-mod": "*"
  }
}
